<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local:true) do |form| %>
<%= form.label :email %>
<%= form.email_field :email %>
<%= form.label :password %>
<%= form.password_field :password %>
<%= form.label :password_confirmation %>
<%= form.password_field :password_confirmation %>
<%= form.submit "Log in" %>
<% end %>
